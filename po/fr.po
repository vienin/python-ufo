#: ../ufo/notify.py:76
msgid "Dismiss"
msgstr "Ignorer"

#: ../ufo/notify.py:116
msgid "New friendship invitation"
msgstr "Nouvelle invitation"

#: ../ufo/notify.py:117
#, python-format
msgid "You have been invited by %(fullname)s to be his/her friend."
msgstr "Vous avez été invité par %(fullname)s à être son ami(e)."

#: ../ufo/notify.py:118
#, python-format
msgid "%(fullname)s wants to be your friend"
msgstr "%(fullname)s désire être votre ami"

#: ../ufo/notify.py:129
#: ../ufo/notify.py:159
msgid "Accept"
msgstr "Accepter"

#: ../ufo/notify.py:136
#: ../ufo/notify.py:166
msgid "Refuse"
msgstr "Refuser"

#: ../ufo/notify.py:143
#: ../ufo/notify.py:173
msgid "Block user"
msgstr "Bloquer l'utilisateur"

#: ../ufo/notify.py:155
msgid "New file sharing request"
msgstr "Nouvelle demande de partage"

#: ../ufo/notify.py:156
#, python-format
msgid "%(fullname)s would like to be in your followers list."
msgstr "%(fullname)s désire suivre vos publications"

#: ../ufo/notify.py:157
#, python-format
msgid "%(fullname)s wants to follow you"
msgstr "%(fullname)s désire suivre vos publications"

#: ../ufo/notify.py:185
msgid "Friendship invitation accepted"
msgstr "Invitation acceptée"

#: ../ufo/notify.py:186
#, python-format
msgid "%(fullname)s has accepted your friendship invitation, you can now share some document with him/her."
msgstr "%(fullname)s a accepté votre invitation à devenir ami. Vous pouvez maintenant partager des fichiers avec lui/elle."

#: ../ufo/notify.py:188
#, python-format
msgid "%(fullname)s has accepted your invitation"
msgstr "%(fullname)s a accepté votre invitation"

#: ../ufo/notify.py:200
msgid "A friendship has been canceled"
msgstr "Vous avez perdu un ami"

#: ../ufo/notify.py:201
#, python-format
msgid "%(fullname)s has removed you from his friend list, you can not access his files any more."
msgstr "%(fullname)s vous a enlevé de sa liste d'ami, vous ne pouvez plus accéder aux fichiers qu'il/elle a partagé avec vous."

#: ../ufo/notify.py:203
#, python-format
msgid "%(fullname)s has canceled his friendship with you"
msgstr "%(fullname)s vous a retiré de sa liste d'amis"

#: ../ufo/notify.py:210
#: ../ufo/notify.py:212
#, python-format
msgid "%(fullname)s has refused your friend request"
msgstr "%(fullname)s ne souhaite pas faire partie de vos amis"

#: ../ufo/notify.py:211
#, python-format
msgid "%(fullname)s would rather be stranger than friends."
msgstr "%(fullname)s ne souhaite plus être votre ami."

#: ../ufo/notify.py:220
msgid "Someone has shared some files with you"
msgstr "%(fullname)s a partagé des fichiers avec vous"

#: ../ufo/notify.py:221
#, python-format
msgid "%(fullname)s has shared the following files with you : %(files)s"
msgstr "%(fullname)s a partagé avec vous les fichiers suivants : %(files)s"

#: ../ufo/notify.py:222
#, python-format
msgid "%(fullname)s has shared some files with you"
msgstr "%(fullname)s a partagé des fichiers avec vous"

#: ../ufo/notify.py:240
msgid "A share has been canceled"
msgstr "Un partage a été annulé"

#: ../ufo/notify.py:241
#, python-format
msgid "%(fullname)s has canceled the share of '%(file)s', you can't access the file any more."
msgstr "%(fullname)s a annulé le partage du fichier '%(file)s'."

#: ../ufo/notify.py:243
#, python-format
msgid "%(fullname)s has canceled a share with you"
msgstr "%(fullname)s a annulé un partage"

